<?xml version="1.0"?> 
<robot name="mouse">
    <!-- Colors --> 
    <material name="black"> 
        <color rgba="0 0 0 0"/>
    </material>
    <material name="blue"> 
        <color rgba="0.2 0.2 0.2 1"/>
    </material>


    <!-- Base Frame of mouse --> 
    <link name="base_link">
        <visual> 
            <geometry> 
                <box size="1 0.5 0.5"/>
            </geometry>
            <material name="blue"/>
        </visual>
        <inertial>
            <mass value="6"/>
            <inertia ixx="0.1" ixy="0" ixz="0" iyy="0.1" iyz="0.0" izz="0.1"/>
        </inertial> 
    </link> 


</robot>
